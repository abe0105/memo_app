<header class="header">
<h1 class="memo-header">メモ一覧</h1>
</header>
 <div class="memo-tuika"><%= link_to 'メモを追加', new_memo_path %></div>
  <div class="memo-title">
    <% @memos.each do |memo| %>
        <p><%= memo.title %></p>
        <p><%= link_to '編集', edit_memo_path(memo) %></p>
        <p><%= link_to '削除', memo_path(memo), method: :delete %></p>
        
    <% end %>
